POSTGRES_PASSWORD=password
POSTGRES_DB=agents_system

# CLI Provider (claude-code-cli or cursor-cli)
CLI_PROVIDER=claude-code-cli

# External APIs
GITHUB_TOKEN=ghp_xxxxx
