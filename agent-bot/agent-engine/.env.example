# ===========================================
# Agent Engine Configuration
# ===========================================

# Server
PORT=8080

# CLI Provider (claude or cursor)
CLI_PROVIDER=claude
MAX_CONCURRENT_TASKS=5
TASK_TIMEOUT_SECONDS=3600

# ===========================================
# Database & Cache
# ===========================================

# Redis URL
REDIS_URL=redis://redis:6379/0

# PostgreSQL URL
DATABASE_URL=postgresql+asyncpg://agent:agent@postgres:5432/agent_system

# ===========================================
# Service URLs
# ===========================================

KNOWLEDGE_GRAPH_URL=http://knowledge-graph:4000

# ===========================================
# API Keys
# ===========================================

# Anthropic API (for Claude CLI)
ANTHROPIC_API_KEY=sk-ant-xxxxxxxxxxxx

# Cursor API (for Cursor CLI)
CURSOR_API_KEY='cur_xxxxxxxxxxxx'

# ===========================================
# Logging
# ===========================================

LOG_LEVEL=INFO
